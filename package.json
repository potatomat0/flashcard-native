{
  "name": "flashcard-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start:localhost": "expo start --localhost --port 8081",
    "typecheck": "tsc --noEmit",
